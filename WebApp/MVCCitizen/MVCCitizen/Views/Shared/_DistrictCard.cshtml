@model District
@{
    ViewData["Title"] = "_DistrictCard";
}
<div class="col-sm-3 col-lg-3 col-md-3 card box-shadow card-header">
    <div class="thumbnail">
        <div class="caption">
            <h5>
                <a asp-controller="Districts" asp-action="Details" asp-route-Id="@Model.Id">@Model.Name</a>
            </h5>
            <h6 class="pull-right">@Model.Description</h6>
        </div>
    </div>
</div>


