@model IEnumerable<District>
@{
    ViewData["Title"] = "Default";
}

<a class="nav-link dropdown-toggle text-light"
   data-toggle="dropdown">Quận/huyện<b class="caret"></b></a>
<div class="dropdown-menu" aria-labelledby="navbarDropdown">
    @foreach (var district in Model)
    {
        <a class="dropdown-item " asp-controller="Districts" asp-action="Index" asp-route-districtName="@district.Name">@district.Name</a>
    }
    <div class="dropdown-divider"></div>
    <a class="dropdown-item" asp-controller="districts" asp-action="Index" asp-route-districtName="">View all districts</a>

</div>
